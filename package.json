{
    "name": "kata",
    "version": "0.1.0",
    "description": "ad",
    "main": "index.js",
    "repository": "repository url",
    "author": "Riccardo Vecchi <rvecchi@byte-code.com>",
    "license": "MIT",
    "engines": {
        "node": "12"
    },
    "scripts": {
        "clean": "shx rm -rf ./dist",
        "format": "tsfmt -r",
        "compile": "ttsc",
        "build": "yarn clean && yarn format && yarn compile",
        "install:db": "serverless dynamodb install",
        "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
        "start": "export AWS_PROFILE=personal && yarn build && node --inspect $(which serverless) offline start --printOutput --stage dev",
        "deploy": "yarn build && serverless deploy --stage dev",
        "pre-commit": "echo 'pre-commit' && yarn format"
    },
    "dependencies": {
        "aws-lambda": "0.1.x",
        "aws-sdk": "^2.589.0",
        "serverless": "1.54.x",
        "uuidv4": "5.0.x",
        "class-validator": "0.11.x"
    },
    "devDependencies": {
        "@types/aws-lambda": "8.10.x",
        "@types/jest": "24.0.x",
        "@types/node": "12.12.x",
        "browserify": "^16.5.0",
        "husky": "3.1.x",
        "jest": "24.9.x",
        "lint-staged": "9.4.x",
        "serverless-dynamodb-local": "0.2.x",
        "serverless-offline": "5.12.x",
        "serverless-plugin-browserifier": "2.1.x",
        "serverless-plugin-browserify": "1.1.x",
        "shx": "0.3.x",
        "ts-jest": "24.2.x",
        "ttypescript": "1.5.x",
        "typescript": "3.7.x",
        "typescript-formatter": "7.2.x",
        "typescript-transform-paths": "1.1.x"
    }
}
